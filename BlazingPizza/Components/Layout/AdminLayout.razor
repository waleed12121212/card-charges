        });

        const cardHeaders = document.querySelectorAll('.connectedSortable .card-header');
        cardHeaders.forEach((cardHeader) => {
            cardHeader.style.cursor = 'move';
        });
    </script>

    <!-- Custom Sidebar Toggle Script -->
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const sidebarToggle = document.querySelector('[data-lte-toggle="sidebar"]');
            const sidebar = document.querySelector('.app-sidebar');
            const body = document.body;

            if (sidebarToggle && sidebar) {
                sidebarToggle.addEventListener('click', function (e) {
                    e.preventDefault();
                    console.log('Sidebar toggle clicked');

                    // Direct approach: toggle sidebar visibility
                    if (sidebar.style.display === 'none' || sidebar.style.marginLeft === '-250px') {
                        // Show sidebar
                        sidebar.style.display = 'block';
                        sidebar.style.marginLeft = '0';
                        body.classList.remove('sidebar-collapse');
                        body.classList.add('sidebar-open');
                        console.log('Sidebar shown');
                    } else {
                        // Hide sidebar
                        sidebar.style.marginLeft = '-250px';
                        body.classList.remove('sidebar-open');
                        body.classList.add('sidebar-collapse');
                        console.log('Sidebar hidden');
                    }
                });
            }

            // Debug: Log initial state
            console.log('Initial body classes:', body.className);
            console.log('Sidebar element:', sidebar);
        });
    </script>

